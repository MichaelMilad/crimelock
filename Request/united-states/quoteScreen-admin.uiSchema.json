              "condition": {}
            }
          },
          {
            "type": "Control",
            "scope": "#/properties/applicant/properties/crimelock/properties/periodFrom",
            "label": "Period From",
            "rule": {
              "effect": "DISABLE",
              "condition": {}
            }
          },
          {
            "type": "Control",
            "scope": "#/properties/applicant/properties/crimelock/properties/periodTo",
            "label": "Period To",
            "rule": {
              "effect": "DISABLE",
              "condition": {}
            }
          },
          {
            "type": "Control",
            "scope": "#/properties/purchase/properties/signDate",
            "label": "Sign Date",
            "options": {
              "minDate": { "days": 60 },
              "minDatePath": "applicant.crimelock.periodFrom"
            }
          },
          {
            "type": "Control",
            "scope": "#/properties/purchase/properties/firmOrderUpload",
            "label": "Firm Order",
            "options": { "format": "radio", "toggle": true }
          },
          {
            "type": "ButtonControl",
            "label": "Bind",
            "scope": "",
            "actions": [
              { "key": "navigate", "data": { "search": "" } },
              { "key": "validate" },
              {
                "key": "invoke",
                "actionId": "9aaad55d-9391-4406-8218-d18f18dd1293",
                "data": { "quoteKey": "$quote-id" }
              },
              {
                "key": "mutate",
                "data": [
                  {
                    "path": "$quote[$quote-id].status",
                    "value": "bound"
                  }
                ]
              }
            ],
            "rule": {
              "effect": "ENABLE",
              "condition": {
                "scope": "#/properties/purchase/properties/firmOrderUpload",
                "schema": { "const": true }
              }
            }
          },
          {
            "type": "ButtonControl",
            "label": "Cancel",
            "scope": "",
            "options": { "style": "default" },
            "actions": [
              { "key": "navigate", "data": { "search": "" } },
              {
                "key": "mutate",
                "data": [
                  {
                    "path": "purchase",
                    "value": { "quote": "" }
                  }
                ]
              },
              { "key": "resetErrors" }
            ]
          }
        ]
      }
