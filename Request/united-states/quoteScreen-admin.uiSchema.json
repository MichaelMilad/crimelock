          "type": "warning",
          "actionButton": {
            "label": "Bind",
            "type": "primary",
            "enabled": true,
            "loading": false,
            "actions": [
              { "key": "navigate", "data": { "search": "?quote-id=$quoteId" } },
              {
                "key": "mutate",
                "data": [
                  {
                    "path": "purchase",
                    "value": {
                      "quote": "$quoteMarket: $quotePremium"
                    }
                  }
                ]
              }
            ]
          },
          "coverageEnabled": false,
          "requote": false,
          "update": false,
          "isQuoteEnabled": true
