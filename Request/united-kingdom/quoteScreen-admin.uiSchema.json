            "loading": false,
            "actions": []
          },
          "coverageEnabled": false,
          "requote": false,
          "update": false,
          "isQuoteEnabled": true
        },
        "binding": {
          "actionButton": {
            "label": "Binding",
            "type": "primary",
            "enabled": false,
            "loading": true,
            "actions": []
          },
          "coverageEnabled": false,
          "requote": false,
          "update": false,
          "isQuoteEnabled": true
        },
        "bound": {
          "label": "Bound",
          "type": "warning",
          "actionButton": {
            "label": "Bound",
            "type": "primary",
            "enabled": false,
            "loading": false,
            "actions": []
          },
          "coverageEnabled": false,
          "requote": false,
          "update": false,
          "isQuoteEnabled": true
        },
        "referred": {
          "label": "Firm Order",
          "type": "warning",
          "actionButton": {
            "label": "Bind",
            "type": "primary",
            "enabled": false,
            "loading": false,
            "actions": []
          },
          "coverageEnabled": false,
          "requote": false,
          "update": false,
          "isQuoteEnabled": true
        },
        "pending-change-approval": {
          "label": "Request Authorized",
          "type": "warning",
          "actionButton": {
            "label": "Result",
            "type": "warning",
            "enabled": true,
            "loading": false,
            "actions": [
              {
                "key": "navigate",
                "data": {
                  "search": "?modal-id=underwriter-recaps"
                }
              }
            ]
          },
          "coverageEnabled": false,
          "requote": false,
          "update": false,
          "isQuoteEnabled": true
        },
        "error": {
          "label": "Error",
          "type": "error",
          "actionButton": {
            "label": "Bind",
            "type": "primary",
            "enabled": false,
            "loading": false,
            "actions": []
          },
          "coverageEnabled": true,
          "requote": true,
          "update": true,
          "isQuoteEnabled": true
        },
        "declined": {
          "label": "Request Declined",
          "type": "error",
          "actionButton": {
            "label": "Bind",
            "type": "primary",
            "enabled": false,
            "loading": false,
            "actions": []
          },
          "coverageEnabled": true,
          "requote": true,
          "update": true,
          "isQuoteEnabled": true
        },
        "pending-bind": {
          "label": "Firm Order",
          "type": "warning",
          "actionButton": {
            "label": "Bind",
            "type": "primary",
            "enabled": true,
            "loading": false,
            "actions": [
              {
                "key": "navigate",
                "data": {
                  "search": "?quote-id=$quoteId"
                }
              },
              {
                "key": "mutate",
                "data": [
                  {
                    "path": "purchase",
                    "value": {
                      "quote": "$quoteMarket: $quotePremium"
                    }
                  }
                ]
              }
            ]
          },
          "coverageEnabled": false,
          "requote": false,
          "update": false,
          "isQuoteEnabled": true
        }
      },
      "markets": {
        "crimelock": {
          "canInitiateUnbind": false,
          "canUpdate": false,
          "canRequote": false,
          "label": "CRIMELOCK"
        }
      },
      "coverages": [
        {
          "key": "limit",
          "type": "MultiValueCoverage"
        },
        {
          "key": "retention",
          "type": "MultiValueCoverage"
        }
      ]
    },
    "elements": [
      {
        "type": "PurchaseLayout",
        "label": "Bind Quote",
        "elements": [
          {
            "type": "Control",
            "scope": "#/properties/purchase/properties/quote",
            "label": "Quote",
            "rule": {
              "effect": "DISABLE",
              "condition": {}
            }
          },
          {
            "type": "Control",
            "scope": "#/properties/applicant/properties/crimelock/properties/periodFrom",
            "label": "Period From",
            "rule": {
              "effect": "DISABLE",
              "condition": {}
            }
          },
          {
            "type": "Control",
            "scope": "#/properties/applicant/properties/crimelock/properties/periodTo",
            "label": "Period To",
            "rule": {
              "effect": "DISABLE",
              "condition": {}
            }
          },
          {
            "type": "Control",
            "scope": "#/properties/purchase/properties/signDate",
            "label": "Sign Date",
            "options": {
              "minDate": {
                "days": 60
              },
              "minDatePath": "applicant.crimelock.periodFrom"
            }
          },
          {
            "type": "Control",
            "scope": "#/properties/purchase/properties/firmOrderUpload",
            "label": "Firm Order",
            "options": {
              "format": "radio",
              "toggle": true
            }
          },
          {
            "type": "ButtonControl",
            "label": "Bind",
            "scope": "",
            "actions": [
              {
                "key": "navigate",
                "data": {
                  "search": ""
                }
              },
              {
                "key": "validate"
              },
              {
                "key": "mutate",
                "data": [
                  {
                    "path": "$quote[$quote-id].status",
                    "value": "binding"
                  }
                ]
              },
              {
                "key": "invoke",
                "actionId": "9aaad55d-9391-4406-8218-d18f18dd1293",
                "data": {
                  "quoteKey": "$quote-id"
                }
              }
            ],
            "rule": {
              "effect": "ENABLE",
              "condition": {
                "scope": "#/properties/purchase/properties/firmOrderUpload",
                "schema": {
                  "const": true
                }
              }
            }
          },
          {
            "type": "ButtonControl",
            "label": "Cancel",
            "scope": "",
            "options": {
              "style": "default"
            },
            "actions": [
              {
                "key": "navigate",
                "data": {
                  "search": ""
                }
              },
              {
                "key": "mutate",
                "data": [
                  {
                    "path": "purchase",
                    "value": {
                      "quote": ""
                    }
                  }
                ]
              },
              {
                "key": "resetErrors"
              }
            ]
          }
        ]
      }
    ]
  }
